{{!-- trips template --}}

<div class="container trips-display page">
  {{#link-to 'trip-create'}}<button class="btn">Create A Trip</button>{{/link-to}}
  <div class="row">
    {{#each model.trips as |eachTrip|}}
    {{trips-snippet trip=eachTrip
                    routeDestroyTrip="destroyTrip"}}
    {{/each}}
  </div>
</div>
